<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.* ?>
<?import javafx.scene.control.* ?>
<?import javafx.scene.layout.*?>

<GridPane fx:controller="org.example.DashboardController"
          xmlns:fx="http://javafx.com/fxml" alignment="TOP_RIGHT" hgap="10" vgap="10"
          stylesheets="@/css/design.css">
    <padding> <Insets top="25" right="25" bottom="25" left="25"/> </padding>

    <HBox spacing="10" alignment="bottom_right"
          GridPane.columnIndex="1" GridPane.rowIndex="4">

        <Button text="Log Out" styleClass="login-button"
                onAction="#logout"/>
    </HBox>

    <HBox spacing="10" alignment="bottom_right"
          GridPane.columnIndex="1" GridPane.rowIndex="5">

        <Button text="Upload" styleClass="login-button"
                onAction="#upload"/>
    </HBox>
</GridPane>
